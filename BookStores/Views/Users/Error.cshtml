
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Enlink - Admin Dashboard Template</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="~/Areas/Style/assets/images/logo/favicon.png">

    <!-- page css -->
    <!-- Core css -->
    <link href="~/Areas/Style/assets/css/app.min.css" rel="stylesheet">

</head>

<body>
    <div class="app">
        <div class="container-fluid">
            <div class="d-flex full-height p-v-20 flex-column justify-content-between">
                <div class="d-none d-md-flex p-h-40">
                    <img src="~/Areas/Style/assets/images/logo/logo.png" alt="">
                </div>
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-md-8 m-h-auto">
                            <div class="text-center m-b-50">
                                <img class="img-fluid w-90" src="~/Areas/Style/assets/images/others/error-2.png" alt="">
                                <h2 class="font-weight-light font-size-30 m-t-60 m-b-20">Xin lỗi, bạn không có quyền truy cập</h2>
                                <p class="w-70 lh-1-8 m-h-auto font-size-17">Vui lòng liên hệ với quản lý để biết thêm chi tiết</p>
                                <a href="@Url.Action("Index","BookStore")" class="btn btn-primary btn-tone">Go Back</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="d-none d-md-flex  p-h-40 justify-content-between">
                    <span class="">© 2023 BookStore</span>
                   
                </div>
            </div>
        </div>
    </div>


    <!-- Core Vendors JS -->
    <script src="~/Areas/Style/assets/js/vendors.min.js"></script>

    <!-- page js -->
    <!-- Core JS -->
    <script src="~/Areas/Style/assets/js/app.min.js"></script>

</body>

</html>